<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="general">
        <field name="pl_size_chart">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Plumrocket\SizeChart\Model\Source\Sizechart</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">260</item>
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Size Chart</item>
                    <item name="default" xsi:type="string">0</item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>